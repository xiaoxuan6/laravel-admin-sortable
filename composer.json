{
    "name": "james.xue/laravel-admin-sortable",
    "description": "Sortable behaviour for eloquent models",
    "type": "library",
    "keywords": [
        "laravel-admin",
        "extension",
        "sort",
        "sortable",
        "eloquent",
        "model",
        "laravel",
        "behaviour"
    ],
    "homepage": "https://github.com/xiaoxuan6/laravel-admin-sortable.git",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaoxuan6",
            "email": "1527736751@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "james.xue/laravel-sortable": "~1.1"
    },
    "require-dev": {
        "encore/laravel-admin": "~1.6",
        "laravel/framework": ">= 5.4",
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Sortable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Sortable\\SortableServiceProvider"
            ]
        }
    },
    "scripts": {
        "fix-style": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no --ansi"
    }
}
